<?xml version="1.0" encoding="UTF-8"?>
<lang>
   <id>en</id>
   <name>English</name>
   <!-- Website title -->
   <title>Rocinante - The Elder Scrolls Online Translation Web App</title>
   <!-- Add-on name -->
   <addon>Add-on</addon>
   <format>
      <!-- Decimal separator -->
      <decimal-mark>.</decimal-mark>
      <!-- Thousands separator -->
      <thousands-mark>,</thousands-mark>
      <!-- Date and time format. See http://php.net/manual/en/class.datetime.php -->
      <date-format>j/n/Y</date-format>
      <time-format>G:i:s</time-format>
      <datetime-format>j/n/Y - G:i:s</datetime-format>
   </format>
   <pager>
      <!-- Page X of Y -->
      <page>Page</page>
      <of>of</of>
   </pager>
   <!-- Gender treating -->
   <gender>
      <male>Male</male>
      <female>Female</female>
   </gender>
   <!-- Type of user -->
   <user-type>
      <male-admin>Admin</male-admin>
      <female-admin>Admin</female-admin>
      <male-advisor>Advisor</male-advisor>
      <female-advisor>Advisor</female-advisor>
      <male-translator>Translator</male-translator>
      <female-translator>Translator</female-translator>
   </user-type>
   <!-- Type of task -->
   <task-type>
      <translation>Translation</translation>
      <revision>Revision</revision>
      <updating>Updating</updating>
      <glossary>Glossary</glossary>
   </task-type>
   <!-- Maintenance is ON -->
   <maintenance-warning>Maintenance mode is ACTIVATED</maintenance-warning>
   <!-- AddOn file info -->
   <addon-file-info>«%1$s» was generated %2$s</addon-file-info>
   <!-- Task building mode -->
   <task-mode>
      <!-- Task contents are assigned by means of selecting strings whose status is 'not translated'
           or 'not revised' -->
      <status value="1">Status</status>
      <!-- Task contents are assigned by means of selecting a number of strings from an offset 
           that includes translated/revised strings -->
      <offset-inc value="2">Range</offset-inc>
      <!-- Task contents are assigned by means of selecting a number of strings from an offset 
           that excludes translated/revised strings -->
      <offset-exc value="3">Consecutive</offset-exc>
   </task-mode>
   <!-- Field validation errors -->
   <validation>
      <error>Please, fix the following errors:</error>
      <is-non-empty>Field «%s» can't be empty.</is-non-empty>
      <is-user-type>Value for field «%s» is unknown.</is-user-type>
      <is-task-type>Value for field «%s» is unknown.</is-task-type>
      <is-min-length>Length of field «%s» must be greater than %d characters.</is-min-length>
      <is-max-length>Length of field «%s» must be less than %d characters.</is-max-length>
      <is-gender>Value for field «%s» is unknown.</is-gender>
      <is-email>Value for field «%s» is not a valid e-mail address.</is-email>
      <is-numeric>Value for field «%s» is not a number.</is-numeric>
      <passwords-mismatch>Passwords don't match.</passwords-mismatch>
      <username-exists>Username «%s» already exists.</username-exists>
      <username-not-found>Username «%s» was not found.</username-not-found>
      <username-has-whitespaces>Username «%s» contains blanks. They aren't allowed.</username-has-whitespaces>
      <string-size-invalid>Number of strings must be greater than zero.</string-size-invalid>
      <string-offset-invalid>Starting string must be greater than zero.</string-offset-invalid>
   </validation>
